{
    "name": "neusta-code-challenge",
    "version": "0.1.2-rc.1",
    "description": "",
    "main": "src/index.ts",
    "scripts": {
        "lint": "tslint --project .",
        "lint:fix": "npm run lint -- --fix",
        "build": "tsc --outDir build",
        "test:unit": "ts-node node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.unit.json",
        "test:e2e": "wait-on tcp:3000 --timeout 10000 && ts-node node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.e2e.json",
        "test": "npm run lint && npm run test:unit && npm run test:e2e",
        "start": "cross-var ts-node $npm_package_main",
        "start:dev": "nodemon --watch src --ext ts --exec \"npm start\"",
        "start:test": "nyc --include src/**/*.ts --extension .ts --require ts-node/register ts-node src/index.ts",
        "preversion": "npm test",
        "postversion": "git push origin master --follow-tags"
    },
    "engines": {
        "node": "^8.9.1",
        "npm": "^5.5.1"
    },
    "homepage": "",
