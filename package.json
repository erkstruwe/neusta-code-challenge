        "express": "^4.16.2",
        "express-form-data": "^2.0.1",
        "helmet": "^3.9.0",
        "highland": "^2.11.1",
        "lodash": "^4.17.4",
        "mongoose": "^4.13.5",
        "ts-node": "^3.3.0",
        "typescript": "^2.6.1",
