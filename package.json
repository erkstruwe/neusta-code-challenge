  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "test:unit": "istanbul cover --include-all-sources --captureExceptions --root src -x \"**/*.spec.js\" node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.unit.json",
    "test:e2e": "jasmine JASMINE_CONFIG_PATH=jasmine.e2e.json",
    "test:parallel": "concurrently --prefix name --names lint,e2e,unit \"npm run lint\" \"npm run test:e2e\" \"npm run test:unit\"",
    "test": "npm run lint && npm run test:e2e && npm run test:unit",
    "start": "node src/index.js",
    "start:dev": "nodemon --watch src",
    "preversion": "npm test",
    "postversion": "git push origin master"
  },
