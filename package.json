        "@types/lodash": "^4.14.85",
        "@types/mongoose": "^4.7.28",
        "@types/node": "^8.0.53",
        "concurrently": "^3.5.1",
        "jasmine": "^2.8.0",
        "nodemon": "^1.12.1",
        "request": "^2.83.0",
        "tslint": "^5.8.0",
        "wait-on": "^2.0.2"
